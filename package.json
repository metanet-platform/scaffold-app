{
  "name": "metanet-scaffold",
  "version": "1.0.0",
  "description": "Production-ready scaffold for building Metanet platform apps with Appwrite backend and Bunny CDN",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "setup": "npm i && node scripts/init-project.js",
    "init:appwrite": "node scripts/setup-appwrite.js",
    "init:collections": "node scripts/setup-collections.js",
    "init:functions": "node scripts/setup-functions.js",
    "init:all": "npm run init:appwrite && npm run init:collections && npm run init:functions",
    "dev": "npm start --workspace=metanet_scaffold_frontend",
    "start": "npm start --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "prebuild": "npm run prebuild --workspaces --if-present",
    "deploy": "dfx deploy",
    "deploy:local": "dfx deploy --network local",
    "deploy:ic": "dfx deploy --network ic",
    "test": "npm test --workspaces --if-present"
  },
  "workspaces": [
    "src/metanet_scaffold_frontend"
  ],
  "keywords": [
    "metanet",
    "appwrite",
    "bunny-cdn",
    "dfinity",
    "react",
    "scaffold",
    "template"
  ],
  "author": "Metanet Platform",
  "license": "MIT",
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/identity": "^2.1.3",
    "@dfinity/principal": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "node-appwrite": "^12.0.1",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "sass": "^1.69.5",
    "tar": "^7.5.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-environment": "^1.1.3"
  }
}
